@prefix ns1: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TM0> ns1:logicalTable [ ns1:sqlQuery """SELECT trip_id, start_time, REPLACE(exact_times, '1', 'true') AS vd788414b4f
FROM 'FREQUENCIES.csv'
WHERE exact_times='1'""" ] ;
    ns1:predicateObjectMap [ ns1:objectMap [ ns1:column "vd788414b4f" ;
                    ns1:datatype xsd:boolean ;
                    ns1:termType ns1:Literal ] ;
            ns1:predicateMap [ ns1:constant <http://vocab.gtfs.org/terms#exactTimes> ] ] ;
    ns1:subjectMap [ ns1:template "http://transport.linkeddata.es/madrid/metro/frequency/{trip_id}-{start_time}" ;
            ns1:termType ns1:IRI ] .

<#TM1> ns1:logicalTable [ ns1:sqlQuery """SELECT start_time, REPLACE(exact_times, '0', 'false') AS vc24f9ed0dc, trip_id
FROM 'FREQUENCIES.csv'
WHERE exact_times='0'""" ] ;
    ns1:predicateObjectMap [ ns1:objectMap [ ns1:column "vc24f9ed0dc" ;
                    ns1:datatype xsd:boolean ;
                    ns1:termType ns1:Literal ] ;
            ns1:predicateMap [ ns1:constant <http://vocab.gtfs.org/terms#exactTimes> ] ] ;
    ns1:subjectMap [ ns1:template "http://transport.linkeddata.es/madrid/metro/frequency/{trip_id}-{start_time}" ;
            ns1:termType ns1:IRI ] .

